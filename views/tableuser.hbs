<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Liste Table</title>
</head>
<body>
  <h1>Liste Table</h1>


    <table border="1">
      <thead>
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Email</th>
          <th>Password</th>
             <th>password_confirm</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}
          <tr>
            <td>{{this.id}}</td>
            <td>{{users.name}}</td>
            <td>{{users.email}}</td>
            <td>{{this.password}}</td>
            <td>{{this.password_confirm}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>

</body>
</html>
